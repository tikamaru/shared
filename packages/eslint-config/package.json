    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-turbo": "2.8.3",
    "prettier": "3.8.1",
    "typescript-eslint": "8.54.0"
  },
  "devDependencies": {
    "eslint": "9.39.2",
    "typescript": "5.9.3"
  }
