{
  "name": "@tikamaru-shared/eslint-config",
  "type": "module",
  "version": "1.0.0",
  "description": "Custom ESLint config by Tikamaru",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
